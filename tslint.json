{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*"
        ]
    },
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "no-empty-interface": false,
        "ban-types": [
            false,
            [
                "Function",
                ""
            ]
        ]
    }
}